<?xml version = '1.0' encoding = 'windows-1252'?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
  <servlet>
    <servlet-name>ConectarMySQL</servlet-name>
    <servlet-class>vistaServlets.ConectarMySQL</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PersonaAlta</servlet-name>
    <servlet-class>vistaServlets.PersonaAlta</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Busquedas</servlet-name>
    <servlet-class>vistaServlets.Busquedas</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ConectarMySQL</servlet-name>
    <url-pattern>/conectarmysql</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PersonaAlta</servlet-name>
    <url-pattern>/personaalta</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Busquedas</servlet-name>
    <url-pattern>/busquedas</url-pattern>
  </servlet-mapping>
</web-app>
